# Credentials
# OpenCart admin credentials
OPENCART_USERNAME=your_admin_username
OPENCART_PASSWORD=your_secure_password
# Correios credentials
CORREIOS_USERNAME=email@example.com
CORREIOS_PASSWORD=your_correios_password
CORREIOS_ADMIN_CODE=12345678

# URLS
OPENCART_PANEL_URL=your_opencart_url
OPENCART_ORDER_URL=your_opencart_order_edit_url
CORREIOS_PANEL_URL=your_correios_dashboard_url
