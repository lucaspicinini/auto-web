# Sender configuration
# City name of the sender
SENDER_CITY=city_name
# Postcard identification
SENDER_POSTCARD=0000000000
# Postal code (CEP)
SENDER_POSTCODE=00000000
# Sender label or company name
SENDER_LABEL=COMPANY_NAME
# Address number
SENDER_ADDRESS_NUMBER=000
# Area code (DDD)
SENDER_DDD=00
# Telephone number without DDD
SENDER_TELEPHONE=00000000
